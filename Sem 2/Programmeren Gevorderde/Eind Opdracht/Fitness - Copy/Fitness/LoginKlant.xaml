<Window x:Class="Fitness.LoginKlant"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fitness"
        mc:Ignorable="d"
        Title="LoginKlant" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Txt_Email" ToolTip="E-mail" HorizontalAlignment="Left" Height="30" Margin="308,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="264" AutomationProperties.HelpText="Username" TextChanged="Txt_Email_TextChanged" />
        <Button x:Name="Btn_Login" Content="Login" HorizontalAlignment="Left" Height="37" Margin="264,250,0,0" VerticalAlignment="Top" Width="111" Click="Btn_Login_Click" />
        <Button x:Name="Btn_Cancel" Content="Cancel" HorizontalAlignment="Left" Height="37" Margin="408,250,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.527,0.53" Click="Btn_Cancel_Click" />
        <Label Content="E-mail:" HorizontalAlignment="Left" Margin="217,171,0,0" VerticalAlignment="Top" />
    </Grid>
</Window>